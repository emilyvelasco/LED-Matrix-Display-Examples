// ISR for display refresh
void display_updater() {
  display.display(70);
}

void setup() {
  display.begin(8);
  display.clearDisplay();
  
  display_ticker.attach(0.002, display_updater);
  yield(); 
}

void drawFrame(uint16_t *frame) {
  display.clearDisplay();
  int imageHeight = 16;
  int imageWidth = 32;
  int counter = 0;
  for (int yy = 0; yy < imageHeight; yy++) {
    for (int xx = 0; xx < imageWidth; xx++) {
      display.drawPixel(xx, yy, frame[counter]);
      counter++;
    }
  }
  delay(500);
}

void loop() {
  drawFrame(eye_frame_1);
  drawFrame(eye_frame_2);
  drawFrame(eye_frame_3);
  drawFrame(eye_frame_4);
  drawFrame(eye_frame_3);
  drawFrame(eye_frame_2);
  drawFrame(eye_frame_1);
  drawFrame(eye_frame_4);
  drawFrame(eye_frame_5);
  drawFrame(eye_frame_6);
  drawFrame(eye_frame_7);
  drawFrame(eye_frame_6);
  drawFrame(eye_frame_5);
  drawFrame(eye_frame_4);
  yield();
}
